{
  "name": "vue-sections",
  "version": "1.0.0",
  "author": "Geeks Solutions info@geeks.solutions (https://www.geeks.solutions)",
  "keywords": [
    "page speed",
    "page building",
    "A/B testing",
    "Content authoring"
  ],
  "description": "A VueJS client to interface with Sections Serverless Page Building and rendering engine",
  "license": "ISC",
  "main": "dist/vue-sections.ssr.js",
  "browser": "dist/vue-sections.esm.js",
  "module": "dist/vue-sections.esm.js",
  "unpkg": "dist/vue-sections.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.21.2",
    "vue-cookies": "^1.7.4",
    "vue-i18n": "^8.22.4",
    "vue-quill-editor": "^3.0.6",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "generate-source-map": "0.0.5",
    "minimist": "^1.2.5",
    "node-sass": "^4.14.1",
    "rollup": "^2.36.1",
    "rollup-plugin-require-context": "^1.0.1",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=12"
  }
}
